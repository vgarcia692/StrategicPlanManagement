<div class="container">
    <h1>Board Key Performance Indicators</h1>
    <br>
    <div>
        <legend>CMI Board KPI's</legend>
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>#</th>
                    <th>KPI</th>
                    <th colspan="3" align="center">2016</th>
                </tr>
                <tr>
                    <th></th>
                    <th></th>
                    <th>Fall</th>
                    <th>Spring</th>
                    <th>Summer</th>
                </tr>
            </thead>
            <tbody ng-repeat="kpi in boardKPIs">
                <tr>
                    <td>{{ $index + 1 }}</td>
                    <td>{{ kpi.kpi }}</td>
                    <td>12%</td><td>50%</td><td>33%</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
 