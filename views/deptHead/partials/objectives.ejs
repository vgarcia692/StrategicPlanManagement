<!-- Nav --!>
<% include ../nav %>
<!-- Sidebar -->
<% include ../sidebar %>
<!-- /#sidebar-wrapper -->


<!-- Page Content -->
<div id="page-content-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1>{{goal.title}}</h1>
				<h2>Objectives</h2>
                <table class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Objective</th>
                        </tr>
                    </thead>
                    <tbody ng-repeat="objective in objectives">
                        <tr>
                            <td>{{ $index + 1 }}</td>
                            <td>{{ objective.objective }}</td>
                        </tr>
                    </tbody>
                </table>
                
            </div>
        </div>
    </div>
</div>
<!-- /#page-content-wrapper -->